{
  "name": "notes",
  "version": "0.0.1",
  "repository": "https://github.com/zimekk/notes.git",
  "author": "Marcin Makarewicz <zimekk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "pod-install ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update": "yarn upgrade-interactive",
    "update:dedupe": "yarn-deduplicate && yarn",
    "update:latest": "yarn upgrade-interactive --latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "react": "18.2.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6",
    "react-native-webview": "^11.22.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-jest": "^28.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "metro-react-native-babel-preset": "^0.71.3",
    "pod-install": "^0.1.37",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^5.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-config|react-native-webview)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "react-native": {
    "styled-components": "styled-components/native"
  }
}
