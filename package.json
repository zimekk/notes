{
  "name": "notes",
  "version": "0.0.1",
  "repository": "https://github.com/zimekk/notes.git",
  "author": "Marcin Makarewicz <zimekk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "pod-install ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update": "yarn upgrade-interactive",
    "update:dedupe": "yarn-deduplicate && yarn",
    "update:latest": "yarn upgrade-interactive --latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.69.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.71.2",
    "pod-install": "^0.1.37",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-native-webview": "^11.22.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^5.0.0"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "engines": {
    "node": ">=14"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
