{
  "name": "notes",
  "version": "0.0.1",
  "repository": "https://github.com/zimekk/notes.git",
  "author": "Marcin Makarewicz <zimekk@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "pod install --project-directory=ios",
    "xed": "xed -b ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "update": "yarn upgrade-interactive",
    "update:dedupe": "yarn-deduplicate && yarn",
    "update:latest": "yarn upgrade-interactive --latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.8",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "react": "18.2.0",
    "react-airplay": "^1.2.0",
    "react-native": "0.69.4",
    "react-native-bootsplash": "^4.3.0",
    "react-native-config": "^1.4.6",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-sound-player": "^0.13.2",
    "react-native-svg": "^12.4.4",
    "react-native-webview": "^11.23.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.17",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.21.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.72.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^5.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community/push-notification-ios|@react-native|react-native|react-native-config|react-native-reanimated|react-native-sound-player|react-native-webview)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "react-native": {
    "styled-components": "styled-components/native"
  }
}
